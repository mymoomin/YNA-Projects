	{func:f_main;
		{when:{arg1};is;error;
			{f_rando}
		;
			{set:pick;{arg1};}
		;}
		{f_sleep}
		{when:{pick};eq;done;
			{!All fine!}
		;
			{!{arg1} wasn't a valid option, defaulting to random!}
			{f_rando}
			{f_sleep}
		;}
	;}
	
